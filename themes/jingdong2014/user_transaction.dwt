      <script>
            /**提交验证表单*/
            function check_sub(){ 
               var rec_id = {$goods.rec_id};
               var num = {$goods.goods_number};
               var num1 = 0 ;
               var frm = document.forms['formReturn'];
              
               /**判断类型**/
               if( !frm['check_return'].checked  && !frm['check_change'].checked ){
                        alert('请选择退换货类型');
                        return  false;
                }
                if( frm['check_return'].checked ){
                       num1 +=  parseInt( $$('num_'+rec_id).value )+ num1;
                        if( num1 > num ){
                            alert('退换货商品数量不能超过已购买商品数量');
                            return false ;
                        }
                }
                if( frm['check_change'].checked ){
                        if( $$('splist_'+rec_id).style.display == 'none'){
                            alert('请添加换货商品');
                            return false;
                        }
                        var num1 = parseInt( frm['attr_num[]'].length )+ num1 ;
                        
                        if( num1> num  ){
                                alert('退换货数量不能超过购买数量');
                                return false;
                        }
                }

                if(frm['parent_id'].value==''){
                    alert('请选择退换货原因');
                    return false;
                }
                if(frm['addressee'].value=''){
                      alert('请填写收件人');
                      return  false;
                  }
                if(frm['address'.value='']){
                     alert('请填写收货地址！');
                     return false ;
                 }
                
                  frm.action = frm.action + '?act=submit_return';
                  frm.submit();  
                  return true;
                
           }
          
            /** 选择退换货原因 */
            function selectCause( val , rec_id ){
                
                if( val >0 ){
                      
                         Ajax.call('user.php?act=ajax_select_cause', 'c_id='+ val +'&rec_id='+rec_id, change_cause_response, 'POST','JSON');

                    }  
                    else { 

                   $$('children_'+rec_id).innerHTML = '';

                    }
               
               
            }
            function change_cause_response(res){
               rec_id = res.rec_id;
               
               $$('children_'+rec_id).innerHTML = res.option;
                
            }
            /**点击退货方法*/
            function  return_app( obj , rec_id ){
              if( $$(obj).checked ){

                    $$('returnid_'+rec_id).style.display="inline-block";
                    $$('retired_'+rec_id).innerHTML = 1;
                    //$$('changeid_'+rec_id).style.display='none';
                    
                }
                else{
                   $$('returnid_'+rec_id).style.display="none";
                        
                    }
            }
            
            /**退货数量控制 **/
            function check_return_num(obj,num ,rec_id){
                        
                 var val = document.getElementById(obj);
                 if( val.value > num){
                        val.value = 1;
                    }
                  if(val.value <= 0 ){
                        val.value = 1;
                    }
                  
                  $$('retired_'+rec_id).innerHTML = val.value;
                    
            }
            /**换货*/
            function return_app1(obj , rec_id ){

                    if( $$(obj).checked ){   
                    $$('changeid_'+rec_id).style.display="inline-block";
                      }
                else{
                      $$('changeid_'+rec_id).style.display="none";
                      $$('splist_'+rec_id).innerTHML = '';
                      $$('splist_'+rec_id).style.display="none";
                        
                    } 
            }
            /**换货获得商品属性*/
                                                  
            function get_spec( rec_id , g_id){
                    
                       // alert(rec_id+"\n"+g_id );
                      
                      document.getElementById('subAttr_'+rec_id).style.display="block";
                      // $$('subAttr_'+rec_id).style.display='block';
                    

                    Ajax.call('goods.php?act=ajax_get_spec', 'rec_id='+rec_id+'&g_id='+g_id, get_pro_response, 'POST','JSON');

                }
             /**获得属性列表回调函数*/   
            function get_pro_response(res){
               
                 document.getElementById('spec_'+res.rec_id).style.display='block';
                 var spec = document.createElement('div');
                 spec.innerHTML=res.spec;
                 document.getElementById('spec_'+res.rec_id).innerHTML = '' ;
                 document.getElementById('spec_'+res.rec_id).appendChild(spec) ;

            }
            /**选择商品属性*/

            function setChange(id,t,attr_id){

                    for (var i = 0; i<t.parentNode.childNodes.length;i++) {
                        if (t.parentNode.childNodes[i].className == 'cattsel' || t.parentNode.childNodes[i].className == 'cattsel catcolor') {
                            t.parentNode.childNodes[i].className = 'catcolor';
                        }
                    }
                    t.className = "cattsel";
                    
                    $$('attr_'+attr_id).setAttribute("value" , id);

            }
            /**
             * 点选可选属性或改变数量时修改商品价格的函数
             */
            function changePrice()
            {
              var attr = getSelectedAttributes(document.forms['ECS_FORMBUY']);
              var qty = document.forms['ECS_FORMBUY'].elements['number'].value;

              Ajax.call('goods.php', 'act=price&id=' + goodsId + '&attr=' + attr + '&number=' + qty, changePriceResponse, 'GET', 'JSON');
            }

           
            /**点击确认换货按钮*/
            function  oncheck_attr( id ,rec_id , goods_id ){

                var num = {$goods.goods_number};   //购买数量
                /**检查退换类型*/
                if( !$$('order_'+rec_id).checked && !$$('change_'+rec_id).checked ){
                            alert("请选择退换类型");
                            return false;
                    }
                    /**退货*/
                  if( $$('order_'+rec_id).checked ){
                       var num1 = parseInt($$('num_'+rec_id).value);
                       if( $$('change_'+rec_id).checked ){
                                num1 += document.getElementsByName('attr_num[]').length;
                                //alert( num1+'\n'+ num);
                                if( num1 >= num ){
                                    alert('退换货数量能超过已买商品数量');   
                                    $$('spec_'+rec_id).style.display = "none";
                                    $$('subAttr_'+rec_id).style.display = 'none';
                                    return false;  
  
                                }
                                $$('retired1_'+rec_id).innerHTML = num1;
                            }
                        if( num1 > num ){
                                 alert('退货数量能超过已买商品数量');    
                                 return false;
                            }
                    }
                    /** 换货*/
                   if( $$('change_'+rec_id).checked ){
                         num2 = 0;
                         if( $$('splist_'+rec_id).style.display == "block"){
                               if( $$('order_'+rec_id).checked ){
                                        num2 += parseInt($$('num_'+rec_id).value);
                                       if( num2 >= num ){
                                            alert("换货数量不能超过已购买数量aaa");
                                            $$('spec_'+rec_id).style.display = "none";
                                            $$('subAttr_'+rec_id).style.display = 'none';
                                            return false;
                                        }
                                }
                               num2 += document.getElementsByName('attr_num[]').length;
                                 if( num2 >= num ){
                                    alert("换货数量不能超过已购买数量");
                                    $$('spec_'+rec_id).style.display = "none";
                                    $$('subAttr_'+rec_id).style.display = "none";
                                    return false;
                                } 
                                
                            } 
                            $$('retired1_'+rec_id).innerHTML = num2+1;

                        }

                   var attr = {$goods.rec_id} ;
                   
                   var attr_id= new Array();
                   
                   var goods_id = {$goods.goods_id};
                
                   /**获得用户所选属性**/
                    var attr = document.forms['form_'+rec_id].getElementsByTagName('input');
             
                    for (var i = 0; i<attr.length;i++) {

                             attr_id[i] = attr[i].value;   
  
                    }
                   Ajax.call('goods.php?act=ajax_set_attr', 'attr_id='+attr_id+'&rec_id='+rec_id+'&goods_id='+goods_id, set_pro_response , 'POST','JSON');
                
            }

            /**确定按钮后返回函数*/
            function set_pro_response(res){
                $$('splist_'+res.rec_id).style.display = "block";
                
                         var str1 = document.createElement('div');
                         str1.innerHTML=res.spec;
                         $$('splist_'+res.rec_id).appendChild(str1);
                
                $$('spec_'+res.rec_id).style.display = "none";
                $$('subAttr_'+res.rec_id).style.display = "none";
                 
            }
            /**删除换货列表*/
            function deleteNum(obj){
                       obj.parentNode.parentNode.parentNode.removeChild( obj.parentNode.parentNode );
                 
            }


