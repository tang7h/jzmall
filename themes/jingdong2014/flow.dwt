<script>
$(function(){
	$(".hd-title b").toggle(function(){
		$(this).parent().next(".hd-content").show();
		$(this).addClass("b");
	},function(){
		$(this).parent().next(".hd-content").hide();
		$(this).removeClass("b");
	});
});
jQuery.fn.cssRadio = function () { 
	jQuery(".wrap-nr label").click( function() {
            var contents = $(this).parent().parent();
			jQuery(".wrap-nr label", contents)
				.each( function() {
					jQuery(this)
						.removeClass()
				});
			jQuery(this)
				.addClass("checked")
			})
		.prev().hide();
}
$(document).ready( function () {
	$("#dx").cssRadio();
});
</script> 
<script type="text/javascript">
function select_Coin(is_coin,coin_name,seller_id)
{
	if(is_coin)
	{
		document.getElementById(coin_name).disabled = false;
		document.getElementById(coin_name).focus();
	}
	else
	{
		document.getElementById(coin_name).disabled = true;
		document.getElementById(coin_name).value=0;
		document.getElementById(coin_name).blur();
		Ajax.call('flow.php?step=use_coin', 'sub_coin=0&seller_id='+seller_id, orderShippingSelectedResponse, 'GET', 'JSON');
	}
}
function change_coin(obj,coin,max_coin,coin_font,seller_id)
{
	if(coin>{$user_wheat_coin})
	{
		alert('您没有足够的有麦币');
		obj.value=0;	
		document.getElementById(coin_font).innerHTML='￥ - 0.00';
		Ajax.call('flow.php?step=use_coin', 'sub_coin=0&seller_id='+seller_id, orderShippingSelectedResponse, 'GET', 'JSON');
		return;
	}
	else if(coin>max_coin)
	{
		alert('您购买的物品只能用'+max_coin+'个以内的有麦币');
		obj.value=0;
		document.getElementById(coin_font).innerHTML='￥ - 0.00';
		Ajax.call('flow.php?step=use_coin', 'sub_coin=0&seller_id='+seller_id, orderShippingSelectedResponse, 'GET', 'JSON');
		return;
	}
	else if(coin<0||(coin.indexOf('.')>=0)||!parseInt(coin))
	{
		alert('有麦币的个数必须为大于等于0的整数');
		obj.value=0;
		document.getElementById(coin_font).innerHTML='￥ - 0.00';
		Ajax.call('flow.php?step=use_coin', 'sub_coin=0&seller_id='+seller_id, orderShippingSelectedResponse, 'GET', 'JSON');
		return;
	}
	else
	{
		var sub_coin=(coin*{$wheat_coin_v});
		document.getElementById(coin_font).innerHTML='￥ - '+sub_coin.toFixed(2);
		Ajax.call('flow.php?step=use_coin', 'sub_coin=' + sub_coin + '&seller_id='+seller_id, orderShippingSelectedResponse, 'GET', 'JSON');
		
	}
}
</script>
